          },
          "phone": {
            "type": "string",
            "description": "Phone",
            "example": "+380962066541"
          },
          "imageURL": {
            "type": "string",
            "description": "Pet image",
            "example": "https://via.placeholder.com/300.png/#FDF7F2/#111111"
          },
          "createdAt": {
            "type": "string",
            "description": "Date of update notice",
            "example": "2023-01-05T15:28:01.426Z"
          },
          
           "updatedAt": {
            "type": "string",
            "description": "Date of update notice",
            "example": "2023-01-05T15:28:01.426Z"
          }
        }
        
      }
  }
}