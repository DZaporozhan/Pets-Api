          "400": {
            "description": "Bad request",
            "content": {}
          },
          "401": {
            "description": "Not authorized",
            "content": {}
          },
          "403": {
            "description": "Please provide a token, no token in authorization header",
            "content": {}
          },
          "404": {
            "description": "Failure! pet with id: ${id} is not found",
            "content": {}
          }
        }
      }
    },
