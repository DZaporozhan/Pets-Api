      "NoticeAddRequest": {
        "type": "object",
        "required": ["category", "title", "sex", "location"],
        "properties": {
          "category": {
            "type": "string",
            "description": "Category by notice on of ['sell', 'in good hands', 'lost/found']",
            "example": "sell"
          },
          "title": {
            "type": "string",
            "description": "The title of the notice",
            "example": "Sell dog"
          },
          "name": {
            "type": "string",
            "description": "Pets name",
            "example": "Gavriil"
          },
          "birthday": {
            "type": "string",
